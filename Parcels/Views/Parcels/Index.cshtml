<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>Prices</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
</head>

<body>
  <div class="container">


    @using Parcels.Models;
    @foreach (Parcel parcel in Model)
    {
      <h3>Your package dimensions:</h3>
      <ul>
        <li>Height: @parcel.Height</li>
        <li>Width: @parcel.Width</li>
        <li>Length: @parcel.Length</li>
        <li>Weight: @parcel.Weight</li>
        <li>Volume: @parcel.GetVol()</li>
        <li>Name: @parcel.Name</li>
        <li>To: @parcel.To</li>
        <li>From: @parcel.From</li>
      </ul>


      <h3>Price to ship your package:</h3>
      <p>$@parcel.CostCalc()</p>
      <hr style="margin-bottom: 100px;">
    }
  </div>
</body>

</html>